    "@types/jest": "^26.0.16",
    "@types/node": "^15.12.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-solid": "^1.0.1",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.1",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "solid-jest": "^0.1.1",
    "solid-js": "^1.0.0",
    "style-vendorizer": "^2.1.1",
    "twind": "^0.16.16",
    "typescript": "~4.3.4",
    "vite-plugin-import": "^0.4.0"
